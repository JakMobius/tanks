{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"src/*": ["src/*"],
			"textures/*": ["src/client/textures/*"],
		},
		"lib": [
			"ES2021",
			"dom"
		],
		"target": "ES2020",
		"module": "ES2020",
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"moduleResolution": "bundler",
		"jsx": "react",
		"incremental": true
	}
}